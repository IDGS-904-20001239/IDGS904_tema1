@model IDGS904_tema1.Models.Alumno

@{
    ViewBag.Title = "Registrar";
}

<h2>Registrar</h2>

@using(Html.BeginForm())
    {
    <div class="form-group">
        <div class="form-horizontal">
            @Html.LabelFor(model => model.nombre, htmlAttributes:
           new { @class = "control-lable col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.nombre, new { htmlAttributes = new { @class = "form-control" } })
            </div>
        </div>

        <div class="form-horizontal">
            @Html.LabelFor(model => model.edad, htmlAttributes:
           new { @class = "control-lable col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.edad, new { htmlAttributes = new { @class = "form-control" } })
            </div>
        </div>

        <div class="form-horizontal">
            @Html.LabelFor(model => model.estaActivo, htmlAttributes:
           new { @class = "control-lable col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.estaActivo, new { htmlAttributes = new { @class = "form-control" } })
            </div>
        </div>

        <div class="form-horizontal">
            @Html.LabelFor(model => model.inscripcion, htmlAttributes:
           new { @class = "control-lable col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.inscripcion, new { htmlAttributes = new { @class = "form-control" } })
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10|">
                <input type="submit" name="name" value="Registrar" />
            </div>
        </div>
    </div>
    } 